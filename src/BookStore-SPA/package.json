{
  "name": "book-store-spa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "debug": "ng serve --port 4201",
    "build": "ng serve --host 192.168.1.55",
    "debugMobile": "ng build",
    "publishGit": "ng build --output-hashing=all --configuration production --output-path ../../docs --base-href www",
    "publishLocal": "ng build --output-hashing=all --configuration local && npm run postbuild",
    "postbuild": "xcopy /s \"./dist\\BookStore-SPA\" \"C:\\inetpub\\LocalYoutube\" /Y",
    "publishExt": "ng build --output-hashing=all --configuration production && npm run postbuildExt",
    "postbuildExt": "xcopy /s \"./dist\\BookStore-SPA\" \"C:\\inetpub\\LocalYoutube External\" /Y",
    "test": "ng test",
    "lint": "ng lint",
    "build-image": "docker build -t nginx-example-app-docker-image .",
    "remove-image": "docker rmi nginx-example-app-docker-image",
    "run-container": "docker run --name nginx-example-app-docker-container -d -p 7878:80 nginx-example-app-docker-image",
    "remove-container": "docker rm -f nginx-example-app-docker-container",
    "containerize": "npm run build-image && npm run run-container",
    "recontainerize": "npm run remove-container && npm run build-image && npm run run-container",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^13.2.3",
    "@angular/animations": "~13.2.2",
    "@angular/cdk": "^13.2.3",
    "@angular/cli": "^14.0.1",
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.2.2",
    "@angular/localize": "^13.2.2",
    "@angular/material": "^13.2.3",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "moment": "^2.29.1",
    "ngx-spinner": "^13.1.1",
    "ngx-toastr": "^13.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5"
  }
}
